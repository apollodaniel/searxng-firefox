{
  "manifest_version": 2,
  "version": "1.0",
  "name": "SearXNG ( Self hosted )",
  "short_name": "SearXNG",
  "author": "Apollo Daniel",
  "description": "Searxng self hosted extension with post method search ( no search history saved on browser ).\nThe searxng must be available on http://localhost:2468.",
  "icons": {
    "32": "assets/searxng.svg",
    "64": "assets/searxng.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "sx, searxng",
      "search_url": "http://localhost:2468/search",
      "search_url_post_params": "q={searchTerms}",
      "suggest_url": "http://localhost:2468/autocompleter",
      "suggest_url_post_params": "q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true,
      "favicon_url": "assets/searxng.svg"
    }
  }
}
